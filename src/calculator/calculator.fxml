<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:controller="calculator.Controller" xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="5" vgap="5" columnConstraints="" >
    <columnConstraints>
        <ColumnConstraints percentWidth="25"/>
        <ColumnConstraints percentWidth="25"/>
        <ColumnConstraints percentWidth="25"/>
        <ColumnConstraints percentWidth="25" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints percentHeight="14" />
        <RowConstraints percentHeight="14" />
        <RowConstraints percentHeight="14" />
        <RowConstraints percentHeight="14" />
        <RowConstraints percentHeight="14" />
        <RowConstraints percentHeight="14" />
        <RowConstraints percentHeight="14" />
    </rowConstraints>
    <TextArea fx:id="mainTextArea" editable="false" GridPane.rowIndex="0" GridPane.rowSpan="2" GridPane.columnIndex="0" GridPane.columnSpan="4"/>

    <TextField fx:id="mainTextField" editable="false" GridPane.rowIndex="2" GridPane.rowSpan="1" GridPane.columnIndex="0" GridPane.columnSpan="4" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "7" onAction="#getSymbol" GridPane.rowIndex="3" GridPane.rowSpan="1" GridPane.columnIndex="0" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "8" onAction="#getSymbol" GridPane.rowIndex="3" GridPane.rowSpan="1" GridPane.columnIndex="1" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "9" onAction="#getSymbol" GridPane.rowIndex="3" GridPane.rowSpan="1" GridPane.columnIndex="2" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "/" onAction="#getSymbol" GridPane.rowIndex="3" GridPane.rowSpan="1" GridPane.columnIndex="3" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "4" onAction="#getSymbol" GridPane.rowIndex="4" GridPane.rowSpan="1" GridPane.columnIndex="0" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "5" onAction="#getSymbol" GridPane.rowIndex="4" GridPane.rowSpan="1" GridPane.columnIndex="1" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "6" onAction="#getSymbol" GridPane.rowIndex="4" GridPane.rowSpan="1" GridPane.columnIndex="2" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "*" onAction="#getSymbol" GridPane.rowIndex="4" GridPane.rowSpan="1" GridPane.columnIndex="3" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "1" onAction="#getSymbol" GridPane.rowIndex="5" GridPane.rowSpan="1" GridPane.columnIndex="0" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "2" onAction="#getSymbol" GridPane.rowIndex="5" GridPane.rowSpan="1" GridPane.columnIndex="1" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "3" onAction="#getSymbol" GridPane.rowIndex="5" GridPane.rowSpan="1" GridPane.columnIndex="2" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "-" onAction="#getSymbol" GridPane.rowIndex="5" GridPane.rowSpan="1" GridPane.columnIndex="3" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "0" onAction="#getSymbol" GridPane.rowIndex="6" GridPane.rowSpan="1" GridPane.columnIndex="0" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "." onAction="#getSymbol" GridPane.rowIndex="6" GridPane.rowSpan="1" GridPane.columnIndex="1" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "=" onAction="#getResult" GridPane.rowIndex="6" GridPane.rowSpan="1" GridPane.columnIndex="2" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

    <Button text = "+" onAction="#getSymbol" GridPane.rowIndex="6" GridPane.rowSpan="1" GridPane.columnIndex="3" GridPane.columnSpan="1" maxHeight="Infinity" maxWidth="Infinity"/>

</GridPane>